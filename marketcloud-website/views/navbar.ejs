<script type="text/javascript">
  //var def = $(".navbar.navbar-default.navbar-fixed-top").css('box-shadow')
  var def = $(".navbar:last-of-type").css('box-shadow');
  $(document).ready(function(){

  if ($(".sub-navbar").length <= 0){

    $(document).on('scroll',function(){
      var scroll = $(window).scrollTop();
      if (scroll === 0) {
        $(".navbar").last().removeClass('has-shadow')
      } else {
        $(".navbar").last().addClass('has-shadow')
      }
    })

  }
  })
</script>