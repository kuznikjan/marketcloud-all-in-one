<script type="text/javascript">
  $(document).ready(function(){


    $(".mobile-sidenav-background-overlay").click(function(){
      $(".mobile-sidenav").css("left","-256px");
      $(this).css('opacity',0.2)
      $(this).removeClass('open');
    })
    //Removing unwanted handlers
    $("button.sidebar-toggle").off();
    //Open the sidebar
    $("button.sidebar-toggle").on("click touch",function(){
      $(".mobile-sidenav-background-overlay").addClass('open');
      //$(".mobile-sidenav").show();
      $(".mobile-sidenav").animate({
        left : "+=256px"
      },300,function(){
        //..
      });
      $(".mobile-sidenav-background-overlay").animate({
        opacity : "0.7"
      },600,function(){
        //..
      });
    })
  })
</script>
