<style type="text/css">
nav.mobile-sidenav{
  display: none;
}
.mobile-sidenav-background-overlay {
  display: none;
}
@media(max-width: 768px){
  .mobile-sidenav-background-overlay {
    position: fixed;
    top:0px; bottom: 0px;left:0px;right: 0px;
    /*background-color: rgba(0,0,0,.4);*/
    background-color: black;
    opacity: 0.2;
    z-index: 1030;
  }
  .mobile-sidenav-background-overlay.open {
  display: block;
  /*background-color: rgba(0,0,0,.7);
  -webkit-transition: background-color 1000ms linear;
  -moz-transition: background-color 1000ms linear;
  -o-transition: background-color 1000ms linear;
  -ms-transition: background-color 1000ms linear;
  transition: background-color 1000ms linear;*/
}
  nav.mobile-sidenav{
    display: block;
    position: fixed;
    left:-260px;
    top:0px;
    bottom: 0px;
    width: 256px;
    background-color: #FFF;
    /*box-shadow: 2px 2px 2px rgba(0,0,0,.4);*/
    z-index: 1031;
  }
  nav.mobile-sidenav ul {
    padding-left: 0px;
  }
  nav.mobile-sidenav ul > li {
    display: block;
    border-bottom: 1px solid #ccc;
    padding:10px 15px;
  }
  nav.mobile-sidenav ul.submenu > li {
    border-bottom: 0px;
  }
  nav.mobile-sidenav ul > li > a {
    font-weight: 500;
  }
  nav.mobile-sidenav ul > li.active > a {
    color: #3498db;
  }
}

</style>
